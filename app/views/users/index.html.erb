 <h3>>スケジュール一覧</h3>
 
 <table>
   <thead>
     <tr>
       <th>ID</th>
       <th>タイトル</th>
       <th>開始日</th>
       <th>終了日</th>
       <th>終日</th>
       <th>スケジュール更新日時</th>
       <th>確認</th>
       <th>編集</th>
       <th>削除</th>
     </tr>
   </thead>
 
   <tbody>
     <% @users.each do |user| %>
       <tr>
         <td><%= user.id %></td>
         <td><%= user.title%></td>
         <td><%= user.start_date %></td>
         <td><%= user.enddate%></td>
         <td><%= user.allday %></td>
         <td><%= user.memo %></td>
         <td><%= user.updated_at %></td>
         <td><%= user.created_at %></td>
         <td>
         <%= link_to "確認", user %>
       </td>
       </tr>
     <% end %>
   </tbody>
 </table>
 
 <p><%= link_to "スケジュール新規登録", :new_user %></p>
